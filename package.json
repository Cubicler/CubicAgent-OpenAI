{
  "name": "@cubicler/cubicagent-openai",
  "version": "2.3.2",
  "description": "Ready-to-deploy OpenAI agent application that integrates OpenAI's language models with Cubicler 2.3.0 using CubicAgentKit 2.3.2",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "cubicagent-openai": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:integration": "vitest --run tests/integration",
    "test:unit": "vitest --run tests/unit",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prestart": "npm run build",
    "publish:manual": "npm run build && npm publish --access public"
  },
  "keywords": [
    "openai",
    "agent",
    "cubicler",
    "mcp",
    "chatgpt",
    "gpt-4",
    "function-calling",
    "deployment"
  ],
  "author": "Cubicler",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cubicler/CubicAgent-OpenAI.git"
  },
  "dependencies": {
    "@cubicler/cubicagentkit": "^2.3.2",
    "openai": "^4.63.0",
    "zod": "^3.23.8",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.14.15",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@eslint/js": "^9.9.0",
    "eslint": "^9.9.0",
    "vitest": "^2.0.5",
    "@vitest/coverage-v8": "^2.0.5",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
}
